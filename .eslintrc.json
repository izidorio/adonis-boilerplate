{
  "env": {
      "commonjs": true,
      "es2020": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "globals": {
    "required": true,
    "use": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "strict": 0,
    "no-param-reassign": [2, { "props": false }]
  }
}
